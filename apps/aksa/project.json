{
  "name": "aksa",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/aksa",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nx/next:build",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "outputPath": "dist/apps/aksa"
      },
      "configurations": {
        "development": {
          "outputPath": "apps/aksa"
        },
        "production": {}
      }
    },
    "serve": {
      "executor": "@nx/next:server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "aksa:build",
        "dev": true
      },
      "configurations": {
        "development": {
          "buildTarget": "aksa:build:development",
          "dev": true
        },
        "production": {
          "buildTarget": "aksa:build:production",
          "dev": false
        }
      },
      "compilerOptions": {
        "paths": {
          "@dashboards/label": ["libs/label/src/index.ts"],
          "@dashboards/button": ["libs/button/src/index.ts"],
          "@dashboards/input": ["libs/input/src/index.ts"]
        }
      }
    },
    "export": {
      "executor": "@nx/next:export",
      "options": {
        "buildTarget": "aksa:build:production"
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "apps/aksa/jest.config.ts"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"]
    }
  },
  "tags": []
}
